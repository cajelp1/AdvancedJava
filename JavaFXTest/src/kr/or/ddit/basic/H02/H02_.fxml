<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox 
maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" 
prefHeight="550.0" prefWidth="550.0" spacing="10.0" 
xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" 
fx:controller="kr.or.ddit.basic.H02.H02_Controller">
   <children>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <ComboBox fx:id="combo" prefWidth="100.0" />
            <TextField fx:id="txtField" />
            <Button fx:id="btn" mnemonicParsing="false" onAction="#btnClicked" text="검색" />
         </children>
      </HBox>
      <TableView fx:id="tableView" prefHeight="500.0" prefWidth="530.0">
        <columns>
          <TableColumn fx:id="zipcode" prefWidth="100.0" text="우편번호" />
          <TableColumn fx:id="sido" prefWidth="75.0" text="시도" />
            <TableColumn fx:id="gugun" prefWidth="100.0" text="구군" />
            <TableColumn fx:id="dong" prefWidth="150.0" text="동" />
            <TableColumn fx:id="bunji" prefWidth="90.0" text="번지" />
        </columns>
      </TableView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
