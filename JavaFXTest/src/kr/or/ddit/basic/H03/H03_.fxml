<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<VBox 
maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" 
prefHeight="300.0" prefWidth="1075.0" spacing="10.0" 
xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" 
fx:controller="kr.or.ddit.basic.H03.H03_Controller">
   <children>
      <HBox alignment="CENTER" spacing="10.0">
         <children>
            <ComboBox fx:id="box1" onAction="#box1Changed" prefWidth="150.0" />
            <ComboBox fx:id="box2" onAction="#box2Changed" prefWidth="150.0" />
         </children>
      </HBox>
      <TableView fx:id="tableview" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="prod_id" prefWidth="100.0" text="PROD_ID" />
          <TableColumn fx:id="prod_name" prefWidth="150.0" text="PROD_NAME" />
            <TableColumn fx:id="prod_lgu" prefWidth="100.0" text="PROD_LGU" />
            <TableColumn fx:id="prod_buyer" prefWidth="100.0" text="PROD_BUYER" />
            <TableColumn fx:id="prod_cost" prefWidth="100.0" text="PROD_COST" />
            <TableColumn fx:id="prod_price" prefWidth="100.0" text="PROD_PRICE" />
            <TableColumn fx:id="prod_sale" prefWidth="100.0" text="PROD_SALE" />
            <TableColumn fx:id="prod_outline" prefWidth="150.0" text="PROD_OUTLINE" />
            <TableColumn fx:id="prod_detail" prefWidth="150.0" text="PROD_DETAIL" />
        </columns>
      </TableView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
